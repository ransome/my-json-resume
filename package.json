{
  "name": "my-json-resume",
  "version": "1.0.1",
  "description": "My Resume",
  "main": "index.js",
  "config": {
    "themeCompact": "--theme=macchiato",
    "themeFull": "--theme=orbit"
  },
  "scripts": {
    "start": "npm-watch",
    "buildHTML": "resume export --resume src/resume.json --format html $npm_package_config_themeFull dist/resume.html"
  },
  "watch": {
    "buildHTML": "{package.json,src/*}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ransome/my-json-resume.git"
  },
  "author": "Ransome Coleman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ransome/my-json-resume/issues"
  },
  "homepage": "https://github.com/ransome/my-json-resume#readme",
  "devDependencies": {
    "jsonresume-theme-eloquent-mod": "^4.23.0",
    "jsonresume-theme-gnap": "^0.0.8",
    "jsonresume-theme-macchiato": "^1.1.0",
    "jsonresume-theme-material": "^1.3.1",
    "jsonresume-theme-mocha-responsive": "^1.1.1",
    "jsonresume-theme-orbit": "^1.1.0",
    "npm-watch": "^0.11.0",
    "resume-cli": "^3.0.7"
  }
}
